<resources>
    //colors for styles
    <attr name="boschColorPrimary" format="color"/>
    <attr name="boschColorHighlighted" format="color"/>
    <attr name="boschColorInverted" format="color"/>
    <attr name="boschColorPrimaryDisabled" format="color"/>
    <attr name="boschColorInvertedDisabled" format="color"/>
    <attr name="hintTextColor" format="color"/>
    <attr name="endIconTint" format="color"/>
<!--    <attr name="endIconDrawable" format="integer"/>-->

    //Bosch Button
    <declare-styleable name="BoschButton">
        <attr name="iconDrawable"/>
        <attr name="boschIcon"/>
    </declare-styleable>

    //Bosch ImageView
    <declare-styleable name="BoschImageView">
        <attr name="iconDrawable"/>
        <attr name="boschIcon"/>
        <attr name="color" format="color"/>
    </declare-styleable>

    //Bosch Slider
    <declare-styleable name="BoschSlider">
        <attr name="vertical" format="boolean"/>
        <attr name="showLabel" format="boolean"/>
        <attr name="enabled"/>
    </declare-styleable>

    <!-- Bosch Label -->
    <declare-styleable name="BoschLabel">
        <attr name="priority" format="enum">
            <enum name="low" value="0"/>
            <enum name="high" value="1"/>
        </attr>
    </declare-styleable>

    <!-- Bosch Dropdown -->
    <declare-styleable name="BoschDropdown">
        <attr name="items" format="reference" />
        <attr name="label"/>
        <attr name="enabled"/>
    </declare-styleable>

    <!-- Bosch TextField -->
    <declare-styleable name="BoschTextField">
        <attr name="label"/>
    </declare-styleable>

    <!-- Bosch Value Modificator -->
    <declare-styleable name="BoschValueModificator">
        <attr name="label"/>
        <attr name="steps" format="float"/>
        <attr name="minValue" format="float"/>
        <attr name="maxValue" format="float"/>
        <attr name="defaultValue" format="float"/>
        <attr name="decimal" format="boolean"/>
        <attr name="enabled"/>
    </declare-styleable>

    <!-- Bosch Slide in Menu -->
    <declare-styleable name="BoschSideNavigation">
        <attr name="boschStyle" format="enum">
            <enum name="Light" value="0"/>
            <enum name="Dark" value="1"/>
            <enum name="SolidFuchsia" value="2"/>
            <enum name="SolidLightgreen" value="3"/>
            <enum name="SolidDarkgreen" value="4"/>
            <enum name="SolidTurquoise" value="5"/>
            <enum name="SolidDarkblue" value="6"/>
            <enum name="SolidViolet" value="7"/>
            <enum name="SolidLightblue" value="8"/>
            <enum name="GradientFuchsiaViolet" value="9"/>
            <enum name="GradientVioletDarkblue" value="10"/>
            <enum name="GradientDarkblueLightblue" value="11"/>
            <enum name="GradientLightblueTurquoise" value="12"/>
            <enum name="GradientTurquoiseLightgreen" value="13"/>
            <enum name="GradientLightgreenDarkgreen" value="14"/>
        </attr>
        <attr name="title" format="string"/>
        <attr name="subTitle" format="string"/>
    </declare-styleable>

    <!-- Bosch Progress -->
    <declare-styleable name="BoschProgressIndicator">
        <attr name="indeterminate" format="boolean"/>
    </declare-styleable>

    <!-- Bosch Gallery -->
    <declare-styleable name="BoschGalleryView">
        <attr name="tileSize" format="enum">
            <enum name="small" value="0"/>
            <enum name="large" value="1"/>
        </attr>
    </declare-styleable>

    //Custom Bosch Attributes
    <attr name="label" format="string"/>
    <attr name="enabled" format="boolean"/>

